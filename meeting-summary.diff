+      if (
+        (packet.buffer[i] === Types.VAR_STRING ||
+          packet.buffer[i] === Types.NULL ||
+          packet.buffer[i] === Types.DOUBLE ||
+          packet.buffer[i] === Types.TINY ||
+          packet.buffer[i] === Types.DATETIME ||
+          packet.buffer[i] === Types.JSON) &&
+        packet.buffer[i + 1] === 0
+      ) {
+        types.push(packet.buffer[i]);
+        packet.skip(2);
+      }
+    }
+
+    packet.skip(1);
+
+    const values = [];
